#using MoDiscoTGG.*
#using AttrCondDefLibrary.*

#rule ClassDec2Class #extends Modifier2NamedElement_Abstract #with MoDiscoTGG

#source { 
	jPackage : Package {
		++ -ownedElements->bodyDeclaration
	}
	
	++ bodyDeclaration : ClassDeclaration
}

#target {
	umlPackage : Package {
		++ -packagedElement->feature
	}
	
	++ feature : Class
}

#correspondence {
	jp2up : JavaPackage2UmlPackage {
		#src->jPackage
		#trg->umlPackage
	}
	++ cd2c : JavaType2UMLType {
		#src->bodyDeclaration
		#trg->feature
	}
}

#attributeConditions {
	eq(bodyDeclaration.name, feature.name)
}
