#using MoDiscoTGG.*
#using AttrCondDefLibrary.*

#rule TypeAccessType2InterfaceRealizationSuperInterface #with MoDiscoTGG

#source {
	classDec : ClassDeclaration {
		-superInterfaces->typeAccess
	}
	
	typeAccess:TypeAccess {
		++-type->jType
	}

	jType:Type
}

#target {
	umlClass : Class {
		-interfaceRealization->interfaceRealization
	}
	
	interfaceRealization:InterfaceRealization {
		++-contract->uType
	}
	
	uType:Interface
}

#correspondence {
	ta2ir:TypeAccess2InterfaceRealization {
		#src->typeAccess
		#trg->interfaceRealization
	}
	
	ne2ne:NamedElement2NamedElement {
		#src->jType
		#trg->uType
	}
}

#attributeConditions {
	
}


