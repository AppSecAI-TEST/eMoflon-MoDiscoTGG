#using MoDiscoTGG.*
#using AttrCondDefLibrary.*

#rule FieldDec2Property #extends FieldDec2Property_Abstract #with MoDiscoTGG

#source { 

	++typeAccess : TypeAccess{
		++-type->jType
	}
	
	
	jType : Type
}

#target {

	++feature : Property{
		++-type->uType
	}
	
	uType : Type
}

#correspondence {

	
	jt2ut : NamedElement2NamedElement{
		#src->jType
		#trg->uType
	}

}


