#using MoDiscoTGG.*
#using AttrCondDefLibrary.*

#abstract #rule MethodDec2Operation_Abstract #extends Modifier2Feature_Abstract #with MoDiscoTGG

#source { 

	classDec : AbstractTypeDeclaration {
		++ -bodyDeclarations->bodyDeclaration
	}
	
	++ bodyDeclaration : MethodDeclaration{
		
	}
}

#target {

	++ feature : Operation{
	}

}

#correspondence {
	
	++ md2o : MethodDec2Operation {
		#src->bodyDeclaration
		#trg->feature
	}
}

#attributeConditions {
	eq(bodyDeclaration.name, feature.name)
}
