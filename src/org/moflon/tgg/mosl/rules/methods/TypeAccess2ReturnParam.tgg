#using MoDiscoTGG.*
#using AttrCondDefLibrary.*

#rule TypeAccess2ReturnParam #with MoDiscoTGG

#source {
	jType:Type

	++typeAccess:TypeAccess{
		++-type -> jType
	}

	method:MethodDeclaration{
		++-returnType -> typeAccess
	}
}

#target {
	uType:uml.Type

	operation:Operation{
		++-ownedParameter -> returnParam
	}

	++returnParam:Parameter{
		++ -type -> uType
	}
}

#correspondence {
	jrt2urt :NamedElement2NamedElement{
		#src->jType
		#trg->uType
	}

	m2o :NamedElement2NamedElement{
		#src->method
		#trg->operation
	}
}

#attributeConditions {
	isReturn(returnParam.direction)
}
